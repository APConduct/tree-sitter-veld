# Test file for function call highlighting

# Function declarations
fn simple_func(x: i32) -> i32
    x + 1
end

fn no_param_func() -> i32
    42
end

# Variable assignments with function calls
let result1 = simple_func(5)
let result2 = no_param_func()
let result3 = simple_func(simple_func(10))

# Function calls with multiple parameters
fn multi_param(a: i32, b: i32, c: i32) -> i32
    a + b + c
end

let result4 = multi_param(1, 2, 3)
let result5 = multi_param(simple_func(1), simple_func(2), simple_func(3))

# Nested function calls
let nested = simple_func(multi_param(1, 2, simple_func(5)))

# Function calls as statements
simple_func(42)
no_param_func()
multi_param(1, 2, 3)

